$TTL 1H
@   IN  SOA ns.cloud.local. admin.cloud.local. (
        2       ; Serial
        1H      ; Refresh
        15M     ; Retry
        1W      ; Expire
        1H      ; Minimum TTL
)

@   IN  NS ns.cloud.local.

ns                  IN A 172.20.0.12

web-frontend-server IN A 10.10.10.10
app-backend         IN A 172.20.0.2
minio               IN A 172.20.0.8
keycloak            IN A 172.20.0.7
